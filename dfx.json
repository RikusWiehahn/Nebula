{
  "canisters": {
    "backend": {
      "candid": "src/backend/backend.did",
      "package": "backend",
      "type": "custom",
      "wasm": "target/wasm32-unknown-unknown/release/backend-opt.wasm",
      "build": "./generate-wasm.sh backend"
    },
    "bucket": {
      "candid": "src/bucket/bucket.did",
      "package": "bucket",
      "type": "custom",
      "wasm": "target/wasm32-unknown-unknown/release/bucket-opt.wasm",
      "build": "./generate-wasm.sh bucket"
    },
    "client_web": {
      "dependencies": [
        "backend"
      ],
      "frontend": {
        "entrypoint": "src/client_web/src/index.html"
      },
      "source": [
        "src/client_web/assets",
        "dist/client_web/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.9.2",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
